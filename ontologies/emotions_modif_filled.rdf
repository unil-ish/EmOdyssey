    <dcterms:subject>Ô Kirkè, qui me montrera le chemin ? Personne n'est jamais arrivé chez Aidés sur une nef noire</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_1776">
    <rdf:type rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#String"/>
