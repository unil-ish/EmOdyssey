    <dcterms:subject>Certes, peut-être le lit d'Odysseus, étant abandonné, reste-t-il en proie aux araignées immondes</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_434">
    <rdf:type rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#String"/>
