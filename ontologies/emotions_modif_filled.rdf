    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#FearPyfeel"/>
    <dcterms:subject>Maintenant, ceins-toi, et que tous ceux-ci nous voient combattre.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_843">
