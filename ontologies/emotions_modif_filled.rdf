    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#FearPyfeel"/>
    <dcterms:subject>Vivant, nous, Akhaiens, nous t'honorions comme un dieu, et, maintenant, tu commandes à tous les morts.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_471">
