    <dcterms:subject>malheureux ! quel malheur allez-vous subir !</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_140">
    <rdf:type rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#String"/>
