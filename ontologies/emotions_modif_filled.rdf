    <owl:disjointWith rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#TerrorSenticnet"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#BlissSenticnet">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#CalmnessSenticnet"/>
    <owl:disjointWith rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#SerenitySenticnet"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_1101">
    <rdf:type rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#String"/>
    <cpo:hasTagtogEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#CalmnessTagtog"/>
    <cpo:hasSenticnetEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#SadnessSenticnet"/>
    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#FearPyfeel"/>
    <dcterms:subject>Prends courage, et ne t'inquiète point de ceci dans ton âme.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://arsemotica.di.unito.it/ontology#BoredomDislike">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
