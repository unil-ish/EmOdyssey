    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#AngryPyfeel"/>
    <dcterms:subject>Mais je te dirai une autre chose ; garde mon conseil dans ton esprit : Fais aborder ta nef dans la chère terre de la patrie, non ouvertement, mais en secret ; car il ne faut point se confier dans les femmes.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_2133">
