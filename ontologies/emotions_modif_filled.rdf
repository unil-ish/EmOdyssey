    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#SadnessPyfeel"/>
    <dcterms:subject>Je ne te fais aucun mal, je ne te dis rien, et je ne t'envie pas à cause des nombreux dons que tu pourras recevoir.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#z">
