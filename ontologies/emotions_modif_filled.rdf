    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#FearPyfeel"/>
    <dcterms:subject>je t'adresserai des voeux tous les jours de ma vie, car tu m'as sauvé, ô vierge !</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nfd06ec1b1ee049e7bec9e178af1c7b88">
