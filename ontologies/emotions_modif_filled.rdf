    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#SadnessPyfeel"/>
    <dcterms:subject>Mais Zeus lui-même mit cette résolution dans mon esprit. Plût aux dieux que j'eusse dû mourir en Aigyptiè et subir alors ma destinée, car d'autres malheurs m'attendaient.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_76">
    <rdf:type rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#String"/>
