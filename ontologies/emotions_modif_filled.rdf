    <rdf:rest rdf:nodeID="N74656d855a004e03b8ac9b637bc010ed"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#SurpriseTagtogStats">
