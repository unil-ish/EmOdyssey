    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#FearPyfeel"/>
    <dcterms:subject>Tèlémakhos, voici qu'une des femmes de la maison, ou Mélanthios, nous expose à un danger terrible.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_363">
