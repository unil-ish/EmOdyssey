    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#CalmnessPyfeel"/>
    <dcterms:subject>Et nous avons pleuré dix-sept jours et dix-sept nuits, dieux immortels et hommes mortels ;</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_1389">
