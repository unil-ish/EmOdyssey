    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#CalmnessPyfeel"/>
    <dcterms:subject>Eurynomè, ne me parle point, tandis que je gémis, de laver et de parfumer mon corps.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nd000abc518094b8b8a67998d31b31530">
