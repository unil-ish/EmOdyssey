    <dcterms:subject>dors, dompté par le sommeil</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_1983">
    <rdf:type rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#String"/>
