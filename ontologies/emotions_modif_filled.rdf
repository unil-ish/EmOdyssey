    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#CalmnessPyfeel"/>
    <dcterms:subject>Malheureuse ! pourquoi, en me priant ardemment, me forces-tu de parler ?</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_927">
