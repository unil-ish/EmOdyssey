    <dcterms:subject>Mais ne vous hâtez point de le renvoyer sans lui faire des présents, car il ne possède rien.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_136">
