    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#JoyPyfeel"/>
    <dcterms:subject>Mes enfants, donnez promptement à Tèlémakhos des chevaux au beau poil, et liez-les au char, afin qu'il fasse son voyage.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_1194">
    <rdf:type rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#String"/>
