    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#CalmnessPyfeel"/>
    <dcterms:subject>Princes et chefs des Phaiakiens, allez à l'agora, afin d'entendre l'étranger qui est arrivé récemment dans la demeure du sage Alkinoos, après avoir erré sur la mer</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_483">
