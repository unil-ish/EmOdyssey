    <dcterms:subject>Il arriva soudainement, et aucun de nous, et même des plus âgés, ne le reconnut.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_1033">
