    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#FearPyfeel"/>
    <dcterms:subject>mais tu as dit une grande parole, et la stupeur me saisit, car deux hommes seuls ne peuvent lutter contre tant de robustes guerriers.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#ISFP">
