    <dcterms:subject>Chienne audacieuse, tu ne peux me cacher ton insolence effrontée que tu payeras de ta tête,</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_1985">
