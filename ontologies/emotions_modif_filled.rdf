    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#SadnessPyfeel"/>
    <dcterms:subject>Je pensais ne plus te revoir depuis que tu es allé sur une nef à Pylos, en secret et contre mon gré, afin de t'informer de ton cher père</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_545">
