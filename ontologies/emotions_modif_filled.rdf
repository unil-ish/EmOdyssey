    <cpo:hasPyfeelEmotion rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#CalmnessPyfeel"/>
    <dcterms:subject>Qu'aucun de vous, prenant ici son repas, ne s'inquiète de ceci, car cette pensée n'est point convenable.</dcterms:subject>
  </rdf:Description>
  <rdf:Description rdf:about="http://pplws.unil.ch/speech/id/speech_1769">
