  <rdf:Description rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#AnnoyanceSenticnet">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://arsemotica.di.unito.it/ontology#Annoyance"/>
    <rdfs:subClassOf rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#AngerSenticnet"/>
