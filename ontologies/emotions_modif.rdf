<?xml version="1.0"?>
<rdf:RDF xmlns="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl"
     xml:base="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://creativecommons.org/ns#"
     xmlns:cpo="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#"
     xmlns:emo="http://arsemotica.di.unito.it/ontology#"
     xmlns:ns1="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:wot="http://xmlns.com/wot/0.1/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:emoart="http://arsemotica.di.unito.it/artifacts#"
     xmlns:ma-ont="http://www.w3.org/ns/ma-ont#"
     xmlns:nif-core="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#">
    <owl:Ontology rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl">
        <owl:versionIRI rdf:resource="https://raw.githubusercontent.com/modellingDH/character-profiling/main/model.owl"/>
        <owl:imports rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core/nif-core.owl"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/lemon/ontolex"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/ma-ont#"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <dc:creator rdf:resource="http://orcid.org/0000-0002-1740-519X"/>
        <dc:creator rdf:resource="http://orcid.org/0000-0002-8323-8764"/>
        <dc:creator rdf:resource="http://orcid.org/0000-0003-2014-0855"/>
        <dc:title>Characters Profiles Ontology (CPO)</dc:title>
        <rdfs:comment>Literary Characters Profile Ontology aligned with FOAF, Lemon, Nif and the ArsEmotica Ontology. In this release, the classes concerning the emotion are also linked to DBpedia according to the Linked Data framework.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.w3.org/2002/07/owl#hasValue -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#hasValue"/>
    


    <!-- http://xmlns.com/foaf/0.1/givenName -->

    <owl:AnnotationProperty rdf:about="http://xmlns.com/foaf/0.1/givenName"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://arsemotica.di.unito.it/artifacts#hasEmotion -->

    <owl:ObjectProperty rdf:about="http://arsemotica.di.unito.it/artifacts#hasEmotion">
        <owl:inverseOf rdf:resource="http://arsemotica.di.unito.it/ontology#EmotionGeneratedByMedia"/>
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
    </owl:ObjectProperty>
    


    <!-- http://arsemotica.di.unito.it/ontology#EmotionGeneratedByMedia -->

    <owl:ObjectProperty rdf:about="http://arsemotica.di.unito.it/ontology#EmotionGeneratedByMedia"/>
    


    <!-- http://arsemotica.di.unito.it/ontology#ExpressOppositeEmotion -->

    <owl:ObjectProperty rdf:about="http://arsemotica.di.unito.it/ontology#ExpressOppositeEmotion">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://arsemotica.di.unito.it/ontology#direct_reference -->

    <owl:ObjectProperty rdf:about="http://arsemotica.di.unito.it/ontology#direct_reference"/>
    


    <!-- http://arsemotica.di.unito.it/ontology#hasEmotionalWord -->

    <owl:ObjectProperty rdf:about="http://arsemotica.di.unito.it/ontology#hasEmotionalWord">
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#Word"/>
    </owl:ObjectProperty>
    


    <!-- http://arsemotica.di.unito.it/ontology#hasOpposite -->

    <owl:ObjectProperty rdf:about="http://arsemotica.di.unito.it/ontology#hasOpposite">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://arsemotica.di.unito.it/ontology#BasicEmotion"/>
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#BasicEmotion"/>
    </owl:ObjectProperty>
    


    <!-- http://arsemotica.di.unito.it/ontology#hasSibling -->

    <owl:ObjectProperty rdf:about="http://arsemotica.di.unito.it/ontology#hasSibling">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://arsemotica.di.unito.it/ontology#BasicEmotion"/>
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#BasicEmotion"/>
    </owl:ObjectProperty>
    


    <!-- http://arsemotica.di.unito.it/ontology#isComposedOf -->

    <owl:ObjectProperty rdf:about="http://arsemotica.di.unito.it/ontology#isComposedOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://arsemotica.di.unito.it/ontology#ComplexEmotion"/>
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#BasicEmotion"/>
    </owl:ObjectProperty>
    


    <!-- http://arsemotica.di.unito.it/ontology#operateNear -->

    <owl:ObjectProperty rdf:about="http://arsemotica.di.unito.it/ontology#operateNear"/>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#denotes_emotion -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#denotes_emotion">
        <rdfs:domain rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#Structure"/>
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
        <rdfs:comment>Object property that make relation between Speech class of the litterary character and Emotion class.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#described_by_word -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#described_by_word">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/ma-ont#Agent"/>
        <rdfs:range rdf:resource="http://lemon-model.net/lemon#Word"/>
        <rdfs:comment>Object property described_by_word make relation between Litterary Character &quot;ma:Agent&quot; and &quot;Word&quot;</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasEmotionalCognitiveFunction -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasEmotionalCognitiveFunction">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
        <rdfs:range rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#MyersBriggsPersonality"/>
        <rdfs:comment>Object Property hasEmotionalCognitiveFunction make relation between Emotion class and Myers Briggs Personality Profile</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasPyfeelEmotion -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasPyfeelEmotion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
        <rdfs:comment>Pyfeel emotion linked to a speech</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasSenticnetEmotion -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasSenticnetEmotion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
        <rdfs:comment>Senticnet emotion linked to a speech</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasTagtogEmotion -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasTagtogEmotion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
        <rdfs:comment>Tagtog emotion linked to a speech</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#has_MyerBriggs_Personality -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#has_MyerBriggs_Personality">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/ma-ont#Agent"/>
        <rdfs:range rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#MyersBriggsPersonality"/>
        <rdfs:comment>Object Property has_MyerBriggs_Personality make relation between Litterary Character &quot;ma:Agent&quot; and Myers Briggs Personality Profile</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#has_Personality -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#has_Personality">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/ma-ont#Agent"/>
        <rdfs:range rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#Personality_Profiles"/>
        <rdfs:comment>Object property has_Personality make relation between Litterary Character &quot;ma:Agent&quot; and NEO-PI &quot;Big 5&quot; Personality Profile</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#isCorrelatedWith -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#isCorrelatedWith">
        <rdfs:domain rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#Personality_Profiles"/>
        <rdfs:range rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#MyersBriggsPersonality"/>
        <rdfs:comment>Object Property isCorrelatedWith make relation between NEO-PI &quot;Big 5&quot; Personality Profile and Myers Briggs Personality Profile</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#isSaidBy -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#isSaidBy">
        <owl:inverseOf rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#said"/>
        <rdfs:comment>Object Property said make relation between the Speech and Litterary Character &quot;ma:Agent&quot;</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#said -->

    <owl:ObjectProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#said">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/ma-ont#Agent"/>
        <rdfs:range rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#Speech"/>
        <rdfs:comment>Object Property said make relation between Litterary Character &quot;ma:Agent&quot; and the Speech</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#topObjectProperty">
        <rdfs:domain rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#String"/>
        <rdfs:range rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/ns/ma-ont#hasCreator -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/ma-ont#hasCreator"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://arsemotica.di.unito.it/ontology#hasScore -->

    <owl:DatatypeProperty rdf:about="http://arsemotica.di.unito.it/ontology#hasScore">
        <rdfs:domain rdf:resource="http://arsemotica.di.unito.it/ontology#BasicEmotion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    </owl:DatatypeProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasAllCommonEmotions -->

    <owl:DatatypeProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasAllCommonEmotions">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Amount of speeches with same emotion between all three different systems (Pyfeel, Tagtog, Senticnet)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasCommonEmotion -->

    <owl:DatatypeProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasCommonEmotion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Amount of same emotions found in different systems (for example Senticnet Joy with Pyfeel Joy)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasTotalEmotions -->

    <owl:DatatypeProperty rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasTotalEmotions">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://arsemotica.di.unito.it/ontology#Emotion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Amount of total emotions found</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#title -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/ma-ont#title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://arsemotica.di.unito.it/ontology#Acceptance -->

    <owl:Class rdf:about="http://arsemotica.di.unito.it/ontology#Acceptance">
        <rdfs:subClassOf rdf:resource="http://arsemotica.di.unito.it/ontology#Trust"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://arsemotica.di.unito.it/ontology#hasScore"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#double">1.0</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://arsemotica.di.unito.it/ontology#Admiration -->

    <owl:Class rdf:about="http://arsemotica.di.unito.it/ontology#Admiration">
        <rdfs:subClassOf rdf:resource="http://arsemotica.di.unito.it/ontology#Trust"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://arsemotica.di.unito.it/ontology#hasScore"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#double">3.0</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://arsemotica.di.unito.it/ontology#Aggressiveness -->

    <owl:Class rdf:about="http://arsemotica.di.unito.it/ontology#Aggressiveness">
        <rdfs:subClassOf rdf:resource="http://arsemotica.di.unito.it/ontology#ComplexEmotion"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://arsemotica.di.unito.it/ontology#isComposedOf"/>
                <owl:someValuesFrom rdf:resource="http://arsemotica.di.unito.it/ontology#Anger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://arsemotica.di.unito.it/ontology#isComposedOf"/>
                <owl:someValuesFrom rdf:resource="http://arsemotica.di.unito.it/ontology#Anticipation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://arsemotica.di.unito.it/ontology#Amazement -->

    <owl:Class rdf:about="http://arsemotica.di.unito.it/ontology#Amazement">
        <rdfs:subClassOf rdf:resource="http://arsemotica.di.unito.it/ontology#Surprise"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://arsemotica.di.unito.it/ontology#hasScore"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#double">3.0</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://arsemotica.di.unito.it/ontology#Anger -->

    <owl:Class rdf:about="http://arsemotica.di.unito.it/ontology#Anger">
        <rdfs:subClassOf rdf:resource="http://arsemotica.di.unito.it/ontology#BasicEmotion"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#hasEmotionalCognitiveFunction"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#extraversion"/>
                            <rdf:Description rdf:about="http://github.com/dpicca/ontologies/literary_characters_psychologica_profiles.owl#thinking"/>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://arsemotica.di.unito.it/ontology#hasOpposite"/>
                <owl:allValuesFrom rdf:resource="http://arsemotica.di.unito.it/ontology#Fear"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://arsemotica.di.unito.it/ontology#hasSibling"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://arsemotica.di.unito.it/ontology#Anticipation"/>
                            <rdf:Description rdf:about="http://arsemotica.di.unito.it/ontology#Disgust"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://arsemotica.di.unito.it/ontology#hasScore"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#double">2.0</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://arsemotica.di.unito.it/ontology#Annoyance -->

    <owl:Class rdf:about="http://arsemotica.di.unito.it/ontology#Annoyance">
        <rdfs:subClassOf rdf:resource="http://arsemotica.di.unito.it/ontology#Anger"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://arsemotica.di.unito.it/ontology#hasScore"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#double">1.0</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://arsemotica.di.unito.it/ontology#Anticipation -->

    <owl:Class rdf:about="http://arsemotica.di.unito.it/ontology#Anticipation">
        <rdfs:subClassOf rdf:resource="http://arsemotica.di.unito.it/ontology#BasicEmotion"/>
